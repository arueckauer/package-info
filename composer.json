{
    "name": "arueckauer/package-info",
    "type": "project",
    "description": "Verifying composer packages requirements of GitHub organizations",
    "keywords": [
        "composer",
        "github",
        "github-api"
    ],
    "homepage": "https://github.com/arueckauer/package-info",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andi RÃ¼ckauer",
            "email": "arueckauer@gmail.com",
            "homepage": "https://arueckauer.github.io/",
            "role": "Project Lead"
        }
    ],
    "require": {
        "php": "^7.4 || ~8.0.0 || ~8.1.0",
        "ext-json": "*",
        "composer/semver": "3.3.2",
        "guzzlehttp/guzzle": "7.4.5",
        "knplabs/github-api": "3.8.0",
        "laminas/laminas-config-aggregator": "1.8.0",
        "laminas/laminas-servicemanager": "3.16.0",
        "laminas/laminas-stdlib": "3.11.0",
        "mezzio/mezzio": "3.11.0",
        "mezzio/mezzio-helpers": "5.8.0",
        "ocramius/package-versions": "2.5.1",
        "symfony/console": "^5.4"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "2.3.0",
        "mikey179/vfsstream": "1.6.11",
        "phpunit/phpunit": "9.5.21",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "4.15.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PackageInfo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PackageInfoTest\\": "test"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "bin/console"
    ],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
