{
    "name": "arueckauer/package-info",
    "type": "project",
    "description": "Verifying requirements of composer packages of a GitHub organization",
    "homepage": "https://github.com/arueckauer/package-info",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andi RÃ¼ckauer",
            "email": "arueckauer@gmail.com",
            "homepage": "https://arueckauer.github.io/",
            "role": "Project Lead"
        }
    ],
    "require": {
        "php": "^7.4 || ~8.0.0",
        "ext-json": "*",
        "composer/semver": "^3.0",
        "guzzlehttp/guzzle": "^7.3",
        "knplabs/github-api": "^3.3",
        "laminas/laminas-config-aggregator": "^1.6",
        "laminas/laminas-servicemanager": "^3.10",
        "laminas/laminas-stdlib": "^3.6",
        "mezzio/mezzio": "^3.6",
        "mezzio/mezzio-helpers": "^5.6",
        "ocramius/package-versions": "^2.0",
        "symfony/console": "^5.3"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "^2.3",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.10"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PackageInfo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PackageInfoTest\\": "test"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
